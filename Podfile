# Public spec repository
source 'https://github.com/CocoaPods/Specs.git'
# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

use_frameworks!

def shared_pods
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'SnapKit'
end

target 'RxPlayground' do
  shared_pods
end

target 'RxPlaygroundUITests' do
  shared_pods
end

target 'RxPlaygroundTests' do
  shared_pods
end

